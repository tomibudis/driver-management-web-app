{
  "name": "codelabs-next-web",
  "version": "0.0.2",
  "description": "This boilerplate is a starter nextjs app to building web platform",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomibudis/codelabs-next-web.git"
  },
  "keywords": [
    "boilerplate",
    "next",
    "app"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmibudis/codelabs-next-web/issues"
  },
  "homepage": "https://github.com/tomibudis/codelabs-next-web#readme",
  "bin": {
    "codelabs-next-web": "bin/cli.js"
  },
  "author": "tomibudis",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "istanbul-badges-readme",
    "extract:colors": "node -r esm scripts/extract-sass-vars.js",
    "check:types": "npx tsc --noEmit",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.25.0",
    "bootstrap-icons": "^1.8.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "esm": "^3.2.25",
    "lodash": "^4.17.21",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^8.0.1",
    "sass": "^1.49.7",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "husky": "^7.0.4",
    "istanbul-badges-readme": "^1.8.1",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  }
}
